<h4>Lista de Nomes</h4>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Nome</th>
    </tr>
  </thead>
  <tbody>
    <% for(let people of listPeople) { %>
      <tr>
        <th scope="row">
          <%= people.id %>
        </th>
        <td>
          <%= people.name %>
        </td>
      </tr>
      <% } %>
  </tbody>
</table>